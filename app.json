{
  "expo": {
    "name": "PlastiSort AI",
    "slug": "plastisort-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#2D5F3F",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.plastisort.ai",
      "infoPlist": {
        "NSCameraUsageDescription": "PlastiSort AI needs access to your camera to scan plastic items and identify their type for proper recycling.",
        "NSPhotoLibraryUsageDescription": "PlastiSort AI needs access to your photo library so you can upload images of plastic items to identify their type."
      }
    },
    "android": {
      "package": "com.plastisort.ai",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#2D5F3F"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow PlastiSort AI to access your camera to scan plastic items."
        }
      ],
      [
        "react-native-fast-tflite",
        {
          "enableCoreMLDelegate": true,
          "enableAndroidGpuLibraries": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "1ce184ac-c2a6-42d1-9af6-f3a00adb1161"
      }
    },
    "owner": "mayank.icu"
  }
}
